[Unit]
Description=TAICLOCK/UDP socket on %i

[Socket]
Backlog=2
BindIPv6Only=both
ListenDatagram=%i:4014
Accept=false
#MaxConnections=16
NoDelay=false
ReceiveBuffer=65536

[Install]
WantedBy=server.target
