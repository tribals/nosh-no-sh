#
# Enable VMWare Guest Additions
#
# This is not a systemd preset.
# It goes in a special nosh system-manager preset directory.
# It is used by the nosh-run-vmware-guest package.
#

enable	vmware-guest.target

enable	vmtoolsd.service
enable	cyclog@vmtoolsd.service
enable	vgauthd.service
enable	cyclog@vgauthd.service
