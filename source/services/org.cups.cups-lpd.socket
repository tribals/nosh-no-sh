[Unit]
Description=Common Unix Printing Service's LPD shim socket

[Socket]
Backlog=2
BindIPv6Only=both
ListenStream=printer
Accept=true
MaxConnections=2
UCSPIRules=true

[Install]
WantedBy=server.target
