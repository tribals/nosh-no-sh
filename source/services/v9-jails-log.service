[Unit]
Description=Combined logger for v9-jail@ and v9-jailed@ services

[Service]
User=%p
SessionLeader=yes
WorkingDirectory=/var/log/sv
ExecStart=cyclog v9-jails/
RestartSec=1

[Install]
WantedBy=workstation.target
